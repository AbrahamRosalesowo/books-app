{{#each books}}
<div class="col-md-4 m-4">
    <div class="card">
        <div class="card-header">
            <h5>{{title}}</h5>
        </div>
        <div class="card-body bg-info" style="color: #303030">
            <p>Author: {{author}}</p>
            <span>ISBN: {{isbn}}</span>
        </div>
    </div>
</div>
{{/each}}